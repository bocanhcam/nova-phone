{
    "name": "bocanhcam/nova-phone",
    "description": "A custom Laravel Nova field to handle phone numbers.",
    "keywords": [
        "laravel",
        "nova",
        "phone",
        "phone number",
        "tel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Bocanhcam\\NovaPhone\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bocanhcam\\NovaPhone\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
